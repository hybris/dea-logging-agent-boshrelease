check process hybris_dea_logging_agent
  with pidfile /var/vcap/sys/run/hybris_dea_logging_agent/dea_logging_agent.pid
  start program "/var/vcap/jobs/hybris_dea_logging_agent/bin/dea_logging_agent_ctl start"
  stop program "/var/vcap/jobs/hybris_dea_logging_agent/bin/dea_logging_agent_ctl stop"
  group vcap
